{
  "name": "fluxxed_up",
  "version": "0.1.0",
  "description": "A react.js -> Flux  utility belt. Handles some basic patterns to save you from boiler plate code. Medium opinionated. ",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run clean && ./node_modules/.bin/babel src/ -d dist/",
    "build-all": "npm run build && npm run build-min",
    "build-min": "./node_modules/.bin/webpack -p dist/index.min.js",
    "clean": "rimraf ./dist/*",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "test-watch": "./node_modules/.bin/karma start --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BJK/fluxxed_up.git"
  },
  "keywords": [
    "react.js",
    "flux.js",
    "react",
    "flux",
    "framework",
    "test"
  ],
  "author": "BJK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BJK/fluxxed_up/issues"
  },
  "homepage": "https://github.com/BJK/fluxxed_up",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-proto-to-assign": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "flux": "^2.0.3",
    "invariant": "^2.1.0",
    "jquery": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chai-plugins": "^0.9.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-es5-shim": "^0.0.4",
    "karma-jquery": "^0.2.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "keymirror": "^0.1.1",
    "mocha": "^3.5.3",
    "object-assign": "^3.0.0",
    "phantomjs": "1.9.8",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "rimraf": "^2.5.2",
    "sinon": "^4.0.0",
    "webpack": "^3.6.0"
  }
}
